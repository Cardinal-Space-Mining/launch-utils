{
    "pragma:enable_preproc": true,
    "pragma:import":
    [
        "bag.json",
        "fg.json",
        "perception.json",
        "robot.json",
        "tag_detection.json",
        "tf.json"
    ],
    "pragma:constants":
    {
        "ARENA_FRAME_ID": "map",
        "ODOM_FRAME_ID": "odom",
        "ROBOT_FRAME_ID": "base_link",
        "LIDAR_FRAME_ID": "lidar_link",

        "LIDAR_HOSTNAME": "10.11.11.3",
        "ROBOT_HOSTNAME": "10.11.11.10",
        "CLIENT_HOSTNAME": "10.11.11.8",

        "LIDAR_SCAN_TOPIC": "/multiscan/lidar_scan",
        "LIDAR_IMU_TOPIC": "/multiscan/imu"
    },

    "preset":
    {
        "pragma:default": null,
        "client":
        {
            "pragma:action_overrides":
            {
                "joy_node": "xbox",
                "robot_status": "enabled",
                "foxglove_bridge": "test",
                "foxglove_gui": "local"
            }
        },
        "mocha":
        {
            "pragma:action_overrides":
            {
                "teleop_control": "enabled",
                "phoenix6_driver": "enabled"
            }
        },
        "frappe":
        {
            "pragma:action_overrides":
            {
                "phoenix5_driver": "enabled"
            }
        }
    }
}
